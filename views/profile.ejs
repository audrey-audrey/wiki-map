<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-zz8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <!-- Bootstrap CSS -->
  <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css" integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS" crossorigin="anonymous">
  <link rel="stylesheet" href="/styles/layout.css" type="text/css"/>

  <link href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
  <script src="http://code.jquery.com/jquery-1.11.1.min.js"></script>
  <link rel="stylesheet" href="/styles/profile.css" type="text/css"/>
  <!------ Include the above in your HEAD tag ---------->

  <title> Midterm App - Login </title>
</head>


<body>
  <%- include('partials/_header') %>

  <div class="container">
    <div class="row profile">
		<div class="col-md-3">
			<div class="profile-sidebar">
				<!-- SIDEBAR USERPIC -->
				<div class="profile-userpic">
					<img src="https://static.change.org/profile-img/default-user-profile.svg" class="img-responsive" alt="">
				</div>
				<!-- END SIDEBAR USERPIC -->
				<!-- SIDEBAR USER TITLE -->
				<div class="profile-usertitle">
					<div class="profile-usertitle-name">
						Marcus Doe
					</div>
					<div class="profile-usertitle-job">
						Developer
					</div>
				</div>
				<!-- END SIDEBAR USER TITLE -->
				<!-- END SIDEBAR BUTTONS -->
				<!-- SIDEBAR MENU -->
				<div class="profile-usermenu">
					<ul class="nav">
						<li class="active">
            <!-- OVERVIEW -->
							<a href="#" class="overview">
							<i class="glyphicon glyphicon-home"></i>
							Overview </a>
            </li>
            <!-- Display profile on-click -->
            <script>
              $(document).ready(function() {
                  $('.overview').click(function() {
                      $(".profile-content").html(
                        "<h1><span style='color: green;'>GeeksforGeeks"+
                        "</span><br>This is the content inside the div"+
                        " after changing innerHTML</h1>");
                  })

              })
          </script>
            <!-- FAVOURITES -->
						<li>
							<a href="#" class="favourites">
							<i class="glyphicon glyphicon-user"></i>
							Favourites </a>
            </li>
            <!-- Display favourites table -->
            <script>
              $(document).ready(function() {
                  $('.overview').click(function() {
                      $(".profile-content").html(
                        "</span><br>This is the content inside the div"+
                        " after changing innerHTML</h1>");
                  })

              })
          </script>
            <!-- CONTRIBUTIONS -->
						<li>
							<a href="#" class="contributions">
							<i class="glyphicon glyphicon-ok"></i>
							Contributions </a>
            </li>
            <!-- Display contribution on click -->
            <script>
              $(document).ready(function() {
                  $('.overview').click(function() {
                      $(".profile-content").html(
                        "<h1><span style='color: green;'>GeeksforGeeks"+
                        "</span><br>This is the content inside the div"+
                        " after changing innerHTML</h1>");
                  })

              })
          </script>
					</ul>
				</div>
				<!-- END MENU -->
			</div>
		</div>
		<div class="col-md-9">
            <div class="profile-content">
			   Some user related content goes here...
            </div>
		</div>
	</div>
</div>

</body>
</html>
